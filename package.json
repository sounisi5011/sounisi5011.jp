    "metalsmith-permalinks": "^2.2.0",
    "metalsmith-postcss2": "^1.0.0",
    "metalsmith-pug-extra": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
