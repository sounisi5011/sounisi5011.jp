{
  "name": "sounisi5011.jp",
  "version": "0.1.2",
  "private": true,
  "description": "sounisi5011.jp's root page",
  "repository": "git@github.com:sounisi5011/sounisi5011.jp.git",
  "license": "UNLICENSED",
  "author": "sounisi5011",
  "scripts": {
    "build": "npm-run-all show-envs --parallel build:*",
    "build:pages": "node ./index.js",
    "clean": "run-p clean:*",
    "clean:cache": "del ./cache/",
    "clean:public": "del ./public/",
    "lint": "eslint ./",
    "show-envs": "./scripts/show-envs.js",
    "start": "run-s build start:server",
    "start:server": "./scripts/get-port.js netlify dev --command='static-server ./public/ -p {}' --targetPort='{}'",
    "test": "run-p lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(package).json|*.{html,css,scss,yaml,yml,toml}": [
      "prettier --write",
      "git add"
    ],
    "**/package.json": [
      "prettier-package-json --write",
      "sort-package-json",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "defaults",
    "> 0.1% in JP"
  ],
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@sounisi5011/metalsmith-add-file-metadata": "file:plugins/metalsmith/add-file-metadata",
    "@sounisi5011/metalsmith-another-source": "file:plugins/metalsmith/another-source",
    "@sounisi5011/metalsmith-asciidoctor": "file:plugins/metalsmith/asciidoctor",
    "@sounisi5011/metalsmith-blankshield": "file:plugins/metalsmith/blankshield",
    "@sounisi5011/metalsmith-clean": "file:plugins/metalsmith/clean",
    "@sounisi5011/metalsmith-comment-matters": "file:plugins/metalsmith/comment-matters",
    "@sounisi5011/metalsmith-copy-convention": "file:plugins/metalsmith/copy-convention",
    "@sounisi5011/metalsmith-download-convention": "file:plugins/metalsmith/download-convention",
    "@sounisi5011/metalsmith-modernizr": "file:plugins/metalsmith/modernizr",
    "@sounisi5011/metalsmith-mustache": "file:plugins/metalsmith/mustache",
    "@sounisi5011/metalsmith-netlify-published-date": "0.2.0",
    "@sounisi5011/metalsmith-page-qr-code-gen": "file:plugins/metalsmith/page-qr-code-gen",
    "@sounisi5011/metalsmith-preload-list": "file:plugins/metalsmith/preload-list",
