{
  "name": "sounisi5011.jp",
  "version": "0.1.2",
  "private": true,
  "description": "sounisi5011.jp's root page",
  "repository": "git@github.com:sounisi5011/sounisi5011.jp.git",
  "license": "UNLICENSED",
  "author": "sounisi5011",
  "scripts": {
    "build": "npm-run-all show-envs clean:public --parallel build:*",
    "build:pages": "node ./index.js",
    "clean": "run-p clean:*",
    "clean:cache": "del ./cache/",
    "clean:public": "del ./public/",
    "lint": "eslint ./",
    "show-envs": "./scripts/show-envs.js",
    "start": "run-s build start:server",
    "start:server": "./scripts/get-port.js netlify dev -c 'static-server ./public/ -p {}' -p '{}'",
    "test": "run-p lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(package).json|*.{html,css,less,yaml,yml,toml}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "sort-package-json",
      "git add"
    ]
  },
  "browserslist": [
    "defaults",
    "> 0.1% in JP"
  ],
  "dependencies": {
    "@babel/preset-env": "^7.5.5",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@sounisi5011/metalsmith-netlify-published-date": "0.2.0",
    "autoprefixer": "^9.6.1",
    "babel-preset-minify": "^0.5.1",
    "cheerio": "^1.0.0-rc.3",
    "convert-svg-to-png": "^0.5.0",
    "core-js": "3",
    "del-cli": "^2.0.0",
    "dialog-polyfill": "^0.5.0",
    "download": "^7.1.0",
    "image-size": "^0.7.4",
    "is-url": "^1.2.4",
    "keyv-file": "^0.1.13",
    "less": "^3.9.0",
    "metalsmith": "^2.3.0",
    "metalsmith-assets-convention": "^2.1.1",
    "metalsmith-babel": "^5.1.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-excerpts": "^1.2.0",
    "metalsmith-ignore": "^1.0.0",
    "metalsmith-in-place": "^4.4.0",
    "metalsmith-permalinks": "^2.2.0",
    "metalsmith-plugin-kit": "^1.0.1",
    "metalsmith-postcss": "^4.2.0",
    "metalsmith-pug-extra": "^1.0.1",
    "mime": "^2.4.3",
    "modernizr": "^3.7.1",
    "mustache": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "postcss-clean": "^1.1.0",
    "qrcode": "^1.3.3",
    "sitemap": "^4.0.2",
    "strict-uri-encode": "^2.0.0",
    "svgo": "^1.2.2",
    "to-ico": "^1.1.5",
    "twitter-text": "^3.0.0"
  },
  "devDependencies": {
    "cross-spawn": "^6.0.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-sort-requires-by-path": "^1.0.2",
    "eslint-plugin-standard": "^4.0.0",
    "get-port": "^5.0.0",
    "husky": "^2.7.0",
    "lint-staged": "^8.1.7",
    "netlify-cli": "^2.11.22",
    "prettier": "^1.17.1",
    "prettier-package-json": "^2.1.0",
    "prettier-plugin-toml": "^0.3.1",
    "sort-package-json": "^1.22.1",
    "static-server": "^2.2.1"
  }
}
